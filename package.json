  "scripts": {
    "build": "nuxt build",
    "build:gencss": "cross-env NODE_ENV=production run-s gen:css build",
    "dev": "nuxt dev",
    "dev:wsl": "CHOKIDAR_USEPOLLING=true nuxt dev",
